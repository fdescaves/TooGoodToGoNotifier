{
  "ConnectionStrings": {
    "TooGoodToGoNotifier": ""
  },
  "TooGoodToGoApiOptions": {
    "BaseUrl": "http://localhost:8080"
  },
  "NotifierOptions": {
    "ThrottleInterval": 5,
    "StartTime": "00:00:00",
    "EndTime": "23:59:59"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "internalLogFile": "Nlog.log",
    "rules": [
      {
        "enabled": true,
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "console-log"
      }
    ]
  }
}

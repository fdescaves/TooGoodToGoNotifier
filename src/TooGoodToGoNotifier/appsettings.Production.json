{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "NLog": {
    "rules": [
      {
        "enabled": true,
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file-log"
      },
      {
        "enabled": true,
        "logger": "*",
        "minLevel": "Warning",
        "writeTo": "email-log"
      }
    ]
  }
}
